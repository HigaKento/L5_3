<h1>ユーザー一覧</h1>
<% @users.each do |e| %>
    <%= e.uid %>：<%= e.pass %>
    <%=link_to '[削除]', user_path(e), data: { turbo_method: :delete } %><br>
<% end %>

<%= link_to "[新規追加]",new_user_path %>
<% if session[:login_uid] %>
    <%= link_to "[ログアウト]", top_logout_path %>
<% else %>
    <%= link_to "[ログイン]", top_main_path %>
<% end %>
<%= link_to "[ツイート一覧]",tweets_path %>